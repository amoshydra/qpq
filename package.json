{
  "name": "qpq",
  "version": "0.2.4",
  "description": "A TUI launcher for frequently used commands",
  "repository": {
    "type": "git",
    "url": "https://github.com/amoshydra/qpq"
  },
  "homepage": "https://github.com/amoshydra/qpq",
  "main": "dist/cli.js",
  "type": "module",
  "bin": {
    "qpq": "./qpq.sh"
  },
  "scripts": {
    "build": "node build.mjs",
    "dev": "tsx src/index.tsx",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepare": "husky"
  },
  "keywords": [
    "qpq",
    "launcher",
    "cli"
  ],
  "author": "",
  "files": [
    "dist",
    "qpq.sh"
  ],
  "license": "MIT",
  "dependencies": {
    "ink": "^6.6.0",
    "ink-scroll-list": "^0.4.1",
    "ink-text-input": "^6.0.0",
    "react": "^19.2.4"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.2.3",
    "@vitest/ui": "^4.0.18",
    "esbuild": "^0.27.3",
    "husky": "^9.1.7",
    "react-dom": "^19.2.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2",
    "vitest": "^4.0.18"
  }
}
