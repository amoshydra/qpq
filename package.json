{
  "name": "qpq",
  "version": "0.0.1",
  "description": "A TUI launcher for frequently used commands",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "qpq": "./dist/qpq.sh"
  },
  "scripts": {
    "build:wrapper": "mkdir -p dist && cp src/scripts/qpq.sh dist/qpq.sh && chmod +x dist/qpq.sh",
    "build": "tsc && pnpm run build:wrapper",
    "dev": "tsx src/index.tsx",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit"
  },
      "keywords": [
        "qpq",
        "launcher",
        "cli"
      ],
    "author": "",
  "license": "ISC",
  "dependencies": {
    "fuse.js": "^7.1.0",
    "ink": "^6.6.0",
    "ink-text-input": "^6.0.0",
    "js-yaml": "^4.1.1",
    "react": "^19.2.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2"
  }
}
